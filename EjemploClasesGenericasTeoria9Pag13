---- Par.cs ---- 
class Par
{
  public int A {get; private set;}
  public int B {get; private set;}

  public Par(int a, int b)
  {
    this.A = a;
    this.B = b;
  }
}



---- program.cs ----
Par p = new Par(1,15);
Console.WriteLine($"A = {par.A} y B = {par.B}");
Console.WriteLine($"A + B = {par.A + par.B} "); 
