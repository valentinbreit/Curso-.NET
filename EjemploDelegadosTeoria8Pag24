------ Auxiliar.cs ------
namespace Teoria8;
class Auxiliar
{
  public void Procesar()
  {
    console.WriteLine(SumaUno(10));
    Console.WriteLine(SumaDos(10));
  }

  int SumaUno(int n) => n + 1;
  int SumaDos(int n) => n + 2;
}


------ Program.cs -----
using Teoria8;

Auxiliar aux = new Auxiliar();
aux.Procesar();
